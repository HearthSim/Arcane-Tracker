<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:tools="http://schemas.android.com/tools"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       android:orientation="vertical"
       tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout"
       android:layout_width="match_parent"
       android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLayout0"
        android:layout_width="0dp"
        android:padding="2dp"
        android:layout_marginTop="4dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/frameLayout1"
        app:layout_constraintTop_toBottomOf="@+id/age"
        app:layout_constraintHorizontal_chainStyle="spread"
        />

    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout0"
        android:layout_marginTop="4dp"
        android:padding="2dp"
        app:layout_constraintRight_toLeftOf="@+id/frameLayout2"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="2dp"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout1"
        android:layout_marginTop="4dp"
        app:layout_constraintRight_toLeftOf="@+id/frameLayout3"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:padding="2dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout2"
        android:layout_marginTop="4dp"
        app:layout_constraintRight_toLeftOf="@+id/frameLayout4"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="0dp"
        android:layout_marginTop="4dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout3"
        android:padding="2dp"
        app:layout_constraintRight_toLeftOf="@+id/frameLayout5"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="0dp"
        android:padding="2dp"
        android:layout_marginTop="4dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout4"
        app:layout_constraintRight_toLeftOf="@+id/frameLayout6"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:padding="2dp"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <TextView
        android:id="@+id/age"
        android:layout_width="wrap_content"
        android:shadowColor="#000"
        android:shadowDx="3"
        android:shadowDy="3"
        android:shadowRadius="3"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:padding="6dp"
        android:background="@drawable/square_outline"
        android:ellipsize="none"
        android:layout_marginTop="8dp"
        android:fadingEdgeLength="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:gravity="center_vertical|left"
        android:requiresFadingEdge="horizontal"
        android:singleLine="true"
        tools:text="1 turn ago"
        android:fontFamily="@font/chunkfive"
        android:textColor="#fff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>


</merge>